@using MDD4All.SpecIF.DataModels.DiagramMetadata
@using MDD4All.UI.BlazorComponents.PropertyPanel
<PropertyTable>

    <PropertyRow TProperty="string"
                 @bind-Value="@DataContext.Title"
                 Name="Title" />

    <PropertyRow TProperty="string"
                 @bind-Value="@DataContext.Revision"
                 Name="Revision" />

    <PropertyRow TProperty="bool"
                 @bind-Value="@DataContext.IsPort"
                 Name="Is Port" />

    <PropertyRow TProperty="bool"
                 @bind-Value="@DataContext.CanResize"
                 Name="Can Resize" />

    <PropertyRow TProperty="bool"
                 @bind-Value="@DataContext.CanRotate"
                 Name="Can Rotate" />

</PropertyTable>